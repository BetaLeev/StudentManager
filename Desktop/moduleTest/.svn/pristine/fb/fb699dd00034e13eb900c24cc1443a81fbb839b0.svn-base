            gdfw_bg.style.left = "42px";
            wyfb_bg.style.left = "42px";
            bgs_bg.style.left = "42px";
            tjkh_bg.style.left = "42px";
            qysj_bg.style.left = "42px";
            zkj_bg.style.left = "42px";
        }else if(screenWidth==450){
            input_box_1.setAttribute('class','input_box_540')
