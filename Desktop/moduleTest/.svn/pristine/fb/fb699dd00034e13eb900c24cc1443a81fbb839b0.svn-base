    // 跳转搜索结果页面
    function fnsearch(){
        var loginstatu = api.getPrefs({sync: true,key:'userloginstatu'});
        if(!loginstatu){
                api.openWin({
                name: 'login',
                url: '../../html/login.html',
            });
        }else{
            var area_value = document.getElementById("area_text")
            var needs_value = document.getElementById("needs_text")
            api.openWin({
                name: 'search_result',
                url: '../../html/search_result.html',
                pageParam: {
                    area_value: area_value.value,
                    needs_value: needs_value.value
                }
            });
            area_value.value = ""
            needs_value.value = ""
            }

