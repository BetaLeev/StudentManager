    apiready = function(){
        var screenWidth = api.winWidth;
        if(screenWidth==540){
            input_box_1.setAttribute('class','input_box_540')
            search_right_1.setAttribute('class','search_right_540')
            input_box_2.setAttribute('class','input_box_540')
