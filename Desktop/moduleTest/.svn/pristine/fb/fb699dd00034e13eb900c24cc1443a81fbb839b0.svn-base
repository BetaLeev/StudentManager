    // 找办公室
    function fnworkingroom(){
        var now = Date.now();
        var appKey = SHA1("A6006620792786"+"UZ"+"B4BE1D4A-18F6-42D1-48CF-DF6C6EA9983E"+"UZ"+now)+"."+now
        api.ajax({
            url: 'https://d.apicloud.com/mcm/api/file?filter={"where":{"name":"workingroom_list.json"},"skip":0,"limit":20}',
            method: 'get',
            cache:false,
            headers:{
                 "X-APICloud-APPId": "A6006620792786",
                 "X-APICloud-APPKey": appKey
            }
        },function(ret1, err){
            if (ret1) {
                if(ret1.length!=0){
                    api.openWin({
                        name: 'workingroom',
                        url: '../../html/workingroom.html',
                        pageParam: {
                            name: ret1[0].url
                        }
                    });
                }else{
                    alert("没数据呀~")
                }
            } else {
                alert(err.msg);
            }
        });

    }
