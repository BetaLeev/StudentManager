            wyfb_bg.style.left = "40px";
            bgs_bg.style.left = "40px";
            tjkh_bg.style.left = "40px";
            qysj_bg.style.left = "40px";
            zkj_bg.style.left = "40px";
        }
        var UIScrollPicture = api.require('UIScrollPicture');
        UIScrollPicture.open({
            rect: {
                x: 0,
                y: 280,
                w: api.winWidth,
                h: 150
            },
            data: {
                paths: [
                    'https://org.modao.cc/uploads4/images/3052/30528343/v2_pmamgr.jpg',
                    'https://org.modao.cc/uploads4/images/3052/30528342/v2_pmamgf.jpg',
                ],
            },
            styles: {
                caption: {
                    height: 35,
                    color: '#E0FFFF',
                    size: 13,
                    bgColor: '#696969',
                    position: 'bottom'
                },
                indicator: {
                    dot: {
                        w: 10,
                        h: 10,
                        r: 5,
                        margin: 5
                    },
                    align: 'center',
                    color: '#FFFFFF',
                    activeColor: '#1296db'
                }
            },
            contentMode: 'scaleToFill',
            interval: 3,
            fixedOn: api.frameName,
            loop: true,
            fixed: false
        }, function(ret, err) {
            if (ret) {
                if (ret.eventType == 'click' && ret.index == 0) {
                    fungoPage('findstage','stagelist.json')
                } else if (ret.eventType == 'click' && ret.index == 1) {
                    gotuijian()
                }
            }
        })
