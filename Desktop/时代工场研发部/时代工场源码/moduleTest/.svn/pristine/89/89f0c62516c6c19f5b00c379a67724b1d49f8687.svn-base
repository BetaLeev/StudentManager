<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
    <title>找空间中间部分</title>
    <link rel="stylesheet" type="text/css" href="../../css/api.css"/>
    <style>
        /*搜索模块css*/
        body{
            width:100%;
            height:auto;
        }
        /*分割线css*/
        .fgx{
            width:100%;
            height:8px;
            background-color:#f8f8f8;
            white-space: pre-wrap;
            border-top: 1px solid #BBB;
            border-bottom: 1px solid #BBB;
        }
        .enwelcome{
            width:100%;
            height:auto;
            font-family:SourceHanSansSC; 
            font-weight:700; font-size:18px; 
            color:rgba(18, 94, 136, 1); font-style:normal; 
            letter-spacing:0px; line-height:27px; text-decoration:none;margin-top: 0px;
            text-indent:1em

        }
        .chnword{
            width:100%;
            height:auto;
            font-family:SourceHanSansSC; 
            font-weight:400; font-size:14px;
            color:rgba(16, 16, 16, 0.3); font-style:normal;
            letter-spacing:0px; line-height:21px; text-decoration:none;
            text-indent:3em
        }
        .asktitle{
            margin-top:20px;
            font-family:SourceHanSansSC;
            font-weight:700; font-size:18px;
            color:rgba(18, 94, 136, 1);
            font-style:normal; letter-spacing:0px; 
            line-height:27px; text-decoration:none;
            text-indent:1em
        }
        .div_top{
            background-color: #f7f4f4;
        }
        form{
            width:99%;
            height:250px;
            margin-top:10px;
        }
        .searchBox{
            width:95%;
            height:50px;
            background-color: white;
            border-radius: 2px;
            margin-left: 10px;
            -webkit-box-shadow:2px 3px 2px #888888;
            box-shadow:2px 3px 2px #888888;
        }
        .search_left{
            width:15px;
            height: 15px;
            margin-top:16px;
            float: left;
            margin-left: 10px;
            margin-right: 20px;
        }
        .search_left img,.search_right img{
            width:15px;
            height: 15px;
        }
        .search_right{
            width:15px;
            height:15px;
            margin-top:16px;
            float: right;
            margin-right: 10px;
        }
        input{-webkit-tap-highlight-color: rgba(0,0,0,0);outline:none;width:70%;
            height:30px;
            margin-top: 10px;
            }
        ::-webkit-input-placeholder{
            color:rgba(16, 16, 16, 0.23);
            font-size: 14px;
            text-align: left;
        }
        .findbtn{
            width:99%;
            height:60px;
            background-color: rgb(18, 94, 136);
            color: rgb(255, 255, 255);
            border-radius:3px;
            margin-left: 5px;
            margin-top:60px;
        }
        .bottom_title{
            width: 100%;
            height: 30px;
            font-size: 20px;
            letter-spacing: 2px;
            font-weight: 400;
            margin-top:10px;
            margin-bottom: 10px;
        }
        .bottom_title span{
            width: auto;
            height: 30px;
            font-family: "微软雅黑";
            margin-left: 10px;
        }
        .bottom_box{
            width: 100%;
            height: 190px;
            overflow: hidden;
            position: relative;
        }
        #div_bottom{
            width: auto;
            height: 200px;
            white-space: nowrap;
            overflow-x: auto;
            overflow-y: hidden;
            padding-left: 10px;
            padding-right: 10px;
            position: relative;
        }
        .itemtuijian{
            width:40%;
            height: 190px;
            display:inline-block;
            position: relative;
        }
        .item_img_box{
            width: 100%;
            height: 100px;
            position: absolute;
            border-radius: 2px;
        }
        .itemtuijian img{
            width:100%;
            height: 100%;
            object-fit: cover;
            border-radius: 2px;
            border-radius:2px;
        }
        .item_bottom_content{
            width: 100%;
            height: 50px;
            position: absolute;
            bottom:30px;
            font-family: "微软雅黑";
        }
        #address_name{
            color: black;
            font-size: 13px;
            font-weight: 500;
            margin-bottom: 3px;
        }
        #item_address_desc{
            color: black;
            font-size: 10px;
            font-weight: 300;
            white-space: initial;
        } 

        .itemtuijian::-webkit-scrollbar{
            display: none;
        }
        .more_span{
            font-size: 12px;
            font-weight: 400;
            letter-spacing: 0px;
            float: right;
            margin-right: 5px;
            margin-top:7px;
            line-height: 37px;
            color:rgba(16, 16, 16, 0.49);
        }
    </style>
</head>
<body>
<!-- 分割线 -->
    <div class="fgx"></div>
    <div class="div_top">
        <p class="enwelcome">Hello,Nice  to  meet  you  here!</p>
        <p class="chnword">已有2635个企业使用智能找地儿找到心仪办公室</p>
        <p class="asktitle">想在哪里办公？</p>
        <!-- 搜索模块 -->
        <form>
            <div class="searchBox">
                <div class="search_left">
                    <img src="../../image/findaddress/search.png">
                </div>
                <input  id = 'searchkey' type="text" name="searchkey" placeholder="试试搜地铁站、城市或国家如:北京" class="searchinput"/>
                <div class="search_right">
                    <img src="../../image/findaddress/voice.png">
                </div>
            </div>
            <button type="submit" class="findbtn" tapmode onclick="fnsearchspace()">智能找地儿</button>
        </form>
    </div>
    <div class="bottom_title">
        <span>猜您想找：</span>
        <span class="more_span" tapmode onclick="fnsdtuijian()">更多>></span>
    </div>
    <div class="bottom_box">
        <div id="div_bottom">
            <!-- 单个推荐样式 -->
            <div class="itemtuijian" tapmode onclick="fngodetail('bj_001')">
                    <div class="item_img_box">
                        <img src="http://a563f04226ce88fd176a.qiniucdn.apicloud-system.com/apicloud/2d2710b7d82c80131462d2e535318cbe.png" />
                    </div>
                    <div class="item_bottom_content">
                        <p id='address_name'>
                            时代工场NO.1
                        </p>
                        <p id='item_address_desc'>
                            随时看&nbsp;&nbsp;地铁便利&nbsp;车位充足
                        </p>
                    </div>
                </div>
                <div class="itemtuijian" tapmode onclick="fngodetail('bj_002')">
                    <div class="item_img_box">
                        <img src="http://a563f04226ce88fd176a.qiniucdn.apicloud-system.com/apicloud/d5548e8b54358f1226745d9201981b39.jpg" />
                    </div>
                    <div class="item_bottom_content">
                        <p id='address_name'>
                            时代工场展腾驿站
                        </p>
                        <p id='item_address_desc'>
                            工商注册&nbsp;&nbsp;CBD顶级商圈
                        </p>
                    </div>
                </div>
                <div class="itemtuijian" tapmode onclick="fngodetail('bj_003')">
                    <div class="item_img_box">
                        <img src="http://a4.qpic.cn/psb?/V10zXZcn49hn7p/q2h4AYHgfrtLF5EOlPt8zVbc3WCPDps.9Cvx*dFd0CY!/c/dL8AAAAAAAAA&ek=1&kp=1&pt=0&bo=QAYJA0AGCQMRADc!&tl=1&tm=1564380000&sce=0-12-12&rf=0-18" />
                    </div>
                    <div class="item_bottom_content">
                        <p id='address_name'>
                            时代工场晨时驿站
                        </p>
                        <p id='item_address_desc'>
                            随时看&nbsp;&nbsp;地铁便利&nbsp;车位充足
                        </p>
                    </div>
                </div>
                <div class="itemtuijian" tapmode onclick="fngodetail('bj_004')">
                    <div class="item_img_box">
                        <img src="http://a3.qpic.cn/psb?/V10zXZcn49hn7p/MgFuHq07W8deY.4eoqhNIeXIICYk.sqUf4zHO6JworY!/c/dL4AAAAAAAAA&ek=1&kp=1&pt=0&bo=QAYJA0AGCQMRADc!&tl=1&tm=1564380000&sce=0-12-12&rf=0-18" />
                    </div>
                    <div class="item_bottom_content">
                        <p id='address_name'>
                            时代工场我本驿站
                        </p>
                        <p id='item_address_desc'>
                            随时看&nbsp;&nbsp;地铁便利&nbsp;CBD商圈
                        </p>
                    </div>
                </div>
        </div>
    </div>
</body>
<script type="text/javascript" src="../../script/api.js"></script>
<script type="text/javascript" src="../../script/SHA1.js"></script>
<script type="text/javascript" src="../../script/APICloud-rest.js"></script>
<script type="text/javascript">
    var input_value = document.getElementById("searchkey")
    var searchkey;
    apiready = function(){
    };
    function fnsearchspace(){
        var loginstatu = api.getPrefs({sync: true,key:'userloginstatu'});
        if(!loginstatu){
            api.openWin({
            name: 'login',
            url: '../../html/login.html',
        });
        }else{
            searchkey = input_value.value;
            api.openWin({
                name: 'match_address_win',
                url: '../../html/match_address.html',
                pageParam: {
                    area_value: searchkey
                }
            });
            input_value.value = ''
        }

    };
    function fngodetail(url){
        var now = Date.now();
        var appKey = SHA1("A6006620792786"+"UZ"+"B4BE1D4A-18F6-42D1-48CF-DF6C6EA9983E"+"UZ"+now)+"."+now
        api.ajax({
            url: 'https://d.apicloud.com/mcm/api/file?filter={"where":{"name":"all_stage.json"},"skip":0,"limit":20}',
            method: 'get',
            cache:false,
            headers:{
                 "X-APICloud-APPId": "A6006620792786",
                 "X-APICloud-APPKey": appKey
            }
        },function(ret1, err){
            if (ret1) {
                if(ret1.length!=0){
                    var userloginstatu = api.getPrefs({sync: true,key:'userloginstatu'});
                    if(userloginstatu){
                        api.openWin({
                            name: 'godetail',
                            url: 'detailmodule.html',
                            pageParam: {
                                name: url,
                                filename:ret1[0].url
                            }
                        });
                    }else{
                        api.openWin({
                            name: 'login',
                            url: '../../html/login.html',
                            pageParam:{
                            name:url,
                            filename:ret1[0].url,
                            page_url:'modules/detailmodule.html'
                        }
                        });
            }
                }else{
                    alert("没数据呀~")
                }
            }else{
                alert(err.msg)
            }
        }); 

    };
    function fnsdtuijian(){
        var now = Date.now();
        var appKey = SHA1("A6006620792786"+"UZ"+"B4BE1D4A-18F6-42D1-48CF-DF6C6EA9983E"+"UZ"+now)+"."+now
        api.ajax({
            url: 'https://d.apicloud.com/mcm/api/file?filter={"where":{"name":"moretuijian.json"},"skip":0,"limit":20}',
            method: 'get',
            cache:false,
            headers:{
                 "X-APICloud-APPId": "A6006620792786",
                 "X-APICloud-APPKey": appKey
            }
        },function(ret1, err){
            if (ret1) {
                if(ret1.length!=0){
                    api.openWin({
                        name: 'sdtuijian',
                        url: '../../html/sdtuijian.html',
                        rect: {
                            x: 0,
                            y: 60,
                            w: api.winWidth,
                            h: api.winHeight
                        },
                        pageParam:{
                            name:ret1[0].url
                        },
                        bounces: false,
                        bgColor: '#ffffff',
                        vScrollBarEnabled: true,
                        hScrollBarEnabled: true
                    });
                }else{
                    alert("没数据呀~")
                }
            } else {
                alert(err.msg);
            }
        });
    }
</script>
</html>