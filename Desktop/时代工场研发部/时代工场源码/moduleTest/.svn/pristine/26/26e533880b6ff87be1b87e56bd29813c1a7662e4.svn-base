<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
    <title>找工位</title>
    <link rel="stylesheet" type="text/css" href="../css/api.css"/>
    <style>
    /*头部css*/
        body{
            padding: 0px;
            margin: 0px;
        }
        #header{
            top:30px;
            width:100%;height:35px;
            display: -webkit-flex;
            display: -ms-flexbox;
            display: -webkit-box;
            display: flex;
            -webkit-flex-flow: row;
            flex-flow: row;
            flex-direction: row;
            justify-content: space-between;
            -webkit-box-pack:justify;
            background-color: white;
            border-bottom: 1px solid #DCDCDC;
        }
        #header .back,#header .title,#header .share{
            display: flex;
            display: -webkit-box;
            display: -webkit-flex;
            width:30%;
        }

        #header .back img{
            width:25px;
            height: 25px;
            margin-left: 10px;
            margin-top: 8px;

        }
        #header .title{
            width:30%;
            -webkit-box-pack:center;
            font-family:SourceHanSansSC; 
            font-weight:400; font-size:18px;
             color:rgb(16, 16, 16); font-style:normal;
            letter-spacing:1px; line-height:42px; text-decoration:none;
        }
         #header .title span{
            width: auto;
            margin:0 auto;
        }
    </style>
</head>
<body>
    <!-- 找驿站头部 -->
    <header id="header">
        <div class="back" tapmode onclick="api.closeWin()" >
            <img src="../image/findaddress/back.png"/>
        </div>
        <div class="title"><span>找工位</span></div>
        <div class="share"></div>
    </header>
</body>
<script type="text/javascript" src="../script/api.js"></script>
<script type="text/javascript">
    var header =  document.getElementById("#header");
    apiready = function(){
        $api.fixStatusBar(header);
        api.openFrame({
            name: 'yizhanmodule',
            url: 'modules/yizhanmodule.html',
            rect: {
                x: 0,
                y: 68,
                w: api.winWidth,
                h: api.winHeight*(6/7)-68
            },
            pageParam: {
                name: api.pageParam.name
            },
            bounces: true,
            vScrollBarEnabled: true,
            hScrollBarEnabled: true,
            fixedOn:api.winName,
        });
        var UIScrollPicture = api.require('UIScrollPicture');
        UIScrollPicture.open({
                    rect: {
                x: 0,
                y: api.winHeight*6/7+1,
                w: api.winWidth,
                h: api.winHeight / 7
            },
            data: {
                paths: ["https://org.modao.cc/uploads4/images/3049/30492422/v2_pm6fa3.jpg","https://org.modao.cc/uploads4/images/3049/30492697/v2_pm6fm8.jpg","https://org.modao.cc/uploads4/images/3049/30492710/v2_pm6fn3.jpg"],
            },
            styles: {
                caption: {
                    height: 35,
                    color: '#E0FFFF',
                    size: 13,
                    bgColor: '#696969',
                    position: 'bottom'
                }
            },
            contentMode: 'scaleToFill',
            interval: 3,
            // fixedOn: api.frameName,
            loop: true,
            fixed: false
        }, function(ret, err) {
            if(ret.eventType=='click'){
                gotuijian()
            }
                
    });
    };
function gotuijian(){
    api.openWin({
            name: 'tuijian',
            url: 'jiankehu.html',
        });
}
</script>
</html>