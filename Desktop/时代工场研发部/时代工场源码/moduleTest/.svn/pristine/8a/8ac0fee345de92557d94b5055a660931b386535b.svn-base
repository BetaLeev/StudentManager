            </div>
        </div>
        <div class="option" tapmode onclick="fnwdqy()">
            <div class="icon icon-qianyue"></div>
            <div class="title">我的签约</div>
            <div class="arrow-panel">
              <div class="text"></div>
            </div>
        </div>
        <div class="option" tapmode onclick="fnsybz()">
            <div class="icon icon-help"></div>
            <div class="title">使用帮助</div>
            <div class="arrow-panel">
              <div class="text"></div>
            </div>
        </div>
        <div class="option" tapmode onclick="fnyjfk()">
            <div class="icon icon-yijianfankui"></div>
            <div class="title">意见反馈</div>
            <div class="arrow-panel">
               <div class="text"></div>
            </div>
        </div>
        <div class="option" tapmode onclick="fntjyj()">
            <div class="icon icon-tuijianyoujiang"></div>
            <div class="title">推荐有奖</div>
            <div class="arrow-panel">
              <div class="text"></div>
            </div>
        </div>
        <div class="option" tapmode onclick="fngrsz()">
            <div class="icon icon-personalsetting"></div>
            <div class="title">个人设置</div>
            <div class="arrow-panel">
               <div class="text"></div>
           </div>
        </div>
        <div class="option" tapmode onclick="fntrouble()">
            <div class="icon icon-personalsetting"></div>
            <div class="title">问题提交</div>
            <div class="arrow-panel">
               <div class="text"></div>
            </div>
        </div>
    </section>
</body>
<script type="text/javascript" src="../../script/api.js"></script>
