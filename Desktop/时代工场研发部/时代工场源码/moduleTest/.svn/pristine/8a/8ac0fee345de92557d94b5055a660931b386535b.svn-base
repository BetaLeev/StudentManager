            <span class="key" tapmode onclick='fnkeying()'></span>
            <span class="buy" tapmode onclick='fnbuying()'></span>
        </div>
        <div id="wrap">
            <span id="ordering" tapmode onclick='fnordering()'>订单</span>
            <span id="pursing" tapmode onclick='fnpursing()'>钱包</span>
            <span id="keying" tapmode onclick='fnkeying()'>钥匙</span>
            <span id="buying" tapmode onclick='fnbuying()'>买单</span>
        </div>
        <div class="option01">
            <div class="icon icon-myorder"></div>
            <div class="title"></div>
            <div class="arrow-panel">
               <div class="text"></div>
            </div>
        </div>
        <!-- 这个模块已经在首页的拓展模块进行调用 -->
        <!-- <div class="option" tapmode onclick="fnwyfbfy()">
