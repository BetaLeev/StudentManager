<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0" />
    <meta name="format-detection" content="telephone=no,email=no,date=no,address=no">
    <title>注册</title>
    <link rel="stylesheet" type="text/css" href="../css/api.css" />
    <style>
    header {
        width: 100%;
        height: 20px;
        position: absolute;
        top:30px;
        background-color: #125E88
    }
    
    header h1 {
        width: 100%;
        height: 50px;
        height: 50px;
        line-height: 50px;
        position: absolute;
        top:0;
        line-height:50px;
        left:40%;
        font-size: 18px;
        color: #fff;
    }
    </style>
</head>

<body>
    <header id="header">
        <!-- <div class="back" tapmode onclick="api.closeWin();"></div> -->
        <h1>会员注册</h1>
    </header>
</body>
<script type="text/javascript" src="../script/api.js"></script>
<script type="text/javascript">
apiready = function() {
    var header = $api.dom("#header");
    $api.fixStatusBar(header);
    var headerH = $api.offset(header).h;
    if(api.pageParam.filename){
        if(api.pageParam.title){
            api.openFrame({
                 name:'registerFrame',
                 url:'./register_frame.html',
                 rect:{
                    x: 0,
                    y: headerH+30,
                    W: "auto",
                    h: "auto"

                 },
                 pageParam:{
                    winname:api.pageParam.winname,
                    name:api.pageParam.name,
                    framename:api.pageParam.framename,
                    filename:api.pageParam.filename,
                    page_url:api.pageParam.page_url,
                    title:api.pageParam.title
                },
                 bgColor: 'rgba(0,0,0,0)',

            });
        }else{
            api.openFrame({
                 name:'registerFrame',
                 url:'./register_frame.html',
                 rect:{
                    x: 0,
                    y: headerH+30,
                    W: "auto",
                    h: "auto"

                 },
                 pageParam:{
                    winname:api.pageParam.winname,
                    name:api.pageParam.name,
                    framename:api.pageParam.framename,
                    filename:api.pageParam.filename,
                    page_url:api.pageParam.page_url
                },
                 bgColor: 'rgba(0,0,0,0)',

            });
        }
    }else{
        api.openFrame({
             name:'registerFrame',
             url:'./register_frame.html',
             rect:{
                x: 0,
                y: headerH+30,
                W: "auto",
                h: "auto"

             },
             pageParam:{
                winname:api.pageParam.winname,
                framename:api.pageParam.framename
             },
             bgColor: 'rgba(0,0,0,0)',

        });
    }

};

</script>

</html>
