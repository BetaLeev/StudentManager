<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="maximum-scale=1.0,minimum-scale=1.0,user-scalable=0,width=device-width,initial-scale=1.0"/>
    <meta name="format-detection" content="telephone=no,email=no,date=no,address=no">
    <title>会话详情</title>
    <link rel="stylesheet" type="text/css" href="../css/api.css"/>
    <style type="text/css">
    body
        body,html{
            width: 100%;
            height:100%;
        }
        #header{
            width: 100%;
            top:10px;
            height: 26px;
            position: fixed;
            text-align: center;
            border-bottom: 2px solid #cdcdcd;
        }
        #header img{
            width:20px;
            float: left;
            position: relative;
            height:20px;
            left: 15px;
        }
        #header span{
            font-weight: 400;
            width:auto;
            font-size: 18px;
            letter-spacing: 3px;
            height:26px;
            line-height:18px;
        }
    </style>
</head>
<body>
    <header id="header">
        <img src="../image/findaddress/back.png" tapmode onclick="api.closeWin()">
        <span id='title'></span>
    </header>
</body>
<script type="text/javascript" src='../script/api.js'></script>
<script type="text/javascript">
    var header = document.getElementById("header");
    var title_obj = document.getElementById("title");
    apiready = function(){
        title_obj.innerHTML = api.pageParam.name;
        $api.fixStatusBar(header);
        api.openFrame({
            name: 'message_detail_frame',
            url: 'modules/message_detail_frame.html',
            rect: {
                x: 0,
                y: 63,
                w: api.winWidth,
                h: api.frameHeight
            },
            bounces: false,
            bgColor: 'rgba(0,0,0,0)',
            vScrollBarEnabled: false,
            hScrollBarEnabled: false
        });
        };
</script>
</html>